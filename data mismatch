	ref table Measure

		measure 'Selection Measure' = ```
				
				 
				var TRevenue = SWITCH(
				            SELECTEDVALUE('Measure Selection'[Measure Order]),
				            0, [Revenue (In Hospital)],
				            1, [Revenue (Home Delivery)],
				            2, [Total Revenue],
				            3, [Total Discount as % of Net Rev]
				            )
				 
				var TParacomp = SWITCH(
				            SELECTEDVALUE('Measure Selection'[Measure Order]),
				            4, [Para Comp % Sales],
				            5, [Total Wages],
				            6, [Revenue per Para Hours],
				            7, [Para Hours per Pet Visit]   
				        )
				
				var THours = SWITCH(
				            SELECTEDVALUE('Measure Selection'[Measure Order]),
				            8, [Regular Hours],
				            9, [Overtime Hours],
				            10, [PTO Hours],
				            11, [Total Hours]  
				        )
				
				var TOTpercet = SWITCH(
				            SELECTEDVALUE('Measure Selection'[Measure Order]),
				            12, [Overtime Hours],
				            13, [Total Hours],
				            14, [OT % Total Hours],
				            15, [Overtime Expense],
				            16, [Total Wages]   
				        )
				
				var TWages = SWITCH(
				            SELECTEDVALUE('Measure Selection'[Measure Order]),
				            17, [Total Wages],
				            18, [Average Daily Wage],
				            19, [Average Overtime Wage],
				            20, [Para Comp BPS]  
				        )
				
				var TPet = SWITCH(
				            SELECTEDVALUE('Measure Selection'[Measure Order]),
				            21, [Total Orders],
				            22, [Average Order (In Hospital)],
				            23, [Pet Medical Visits]  
				        )
				 
				        Return
				        SWITCH(
				            SELECTEDVALUE( 'Measure Selection'[Category]) ,
				             "TOTAL REVENUE", TRevenue,  
				             "PARA COMP % OF SALES", TParacomp,
				             "TOTAL HOURS",THours,
				             "OT % OF TOTAL HOURS",TOTpercet,
				             "TOTAL WAGES",TWages,
				             "PET MEDICAL VISITS",TPet)
				
				/*SWITCH(
							SELECTEDVALUE('Measure Selection'[Measure Order]),
				            0, [Revenue (In Hospital)],
				            1, [Revenue (Home Delivery)],
