				            3, [Total Discount as % of Net Rev],
				            4, [Para Comp % Sales],
				            5, [Total Wages],
				            6, [Revenue per Para Hours],
				            7, [Para Hours per Pet Visit],
				            8, [Regular Hours],
				            9, [Overtime Hours],
				            10, [PTO Hours],
				            11, [Total Hours],
				            12, [Overtime Hours],
				            13, [Total Hours],
				            14, [OT % Total Hours],
				            15, [Overtime Expense],
				            16, [Total Wages],
				            17, [Total Wages],
				            18, [Average Daily Wage],
				            19, [Average Overtime Wage],
				            20, [Para Comp BPS],
