Para Comp % Sales = ROUND(DIVIDE ( [Total Wages], [Total Revenue] ),3)

Total Wages =     IF (
        SELECTEDVALUE ( 'Hour Type'[Type] ) = "Estimate",
        [Total Wages Estimate],
        [Total Wages Actual]
    )


Total Revenue = CALCULATE(SUM(Revenue[Net Revenue])) 

